{
  "name": "@agotao/papi",
  "version": "1.0.0",
  "scripts": {
    "build": "tsup src/index.ts --format cjs && cp src/index.html dist",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsup src/index.ts --format cjs --watch --onSuccess \"node dist/index.js\" && cp src/index.html dist",
    "lint": "tsc --noEmit && TIMING=1 eslint \"src/**/*.ts*\"",
    "start": "node dist/index.js"
  },
  "keywords": [],
  "author": "Nestor Mamani",
  "license": "ISC",
  "dependencies": {
    "@hapi/boom": "^10.0.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "express": "^4.18.2",
    "express-joi-validation": "^5.0.1",
    "joi": "^17.7.0",
    "morgan": "^1.10.0",
    "@prisma/client": "^4.6.1"
  },
  "devDependencies": {
    "prisma": "^4.6.1",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.11.18",
    "eslint": "^8.28.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.3"
  }
}
